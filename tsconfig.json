{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "build",
    "rootDir": ".",
    "sourceMap": true,
    "strictPropertyInitialization": false
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
